<% include ../Partials/References %>

<head>
	<title></title>
</head>
<body>
	<ol class="breadcrumb">
		<li><a href="/">Home</a></li>
		<li><a href="/Companies">Companies</a></li>
		<li class="active"><%= Company.Name %></li>
	</ol>

	<h1>Upgrade to use Handlebars</h1>
	<div class="container">
		<form>
			<div class="form-group">
				<label for="Name">Name</label>
				<input type="text" class="form-control" id="" aria-describedby="emailHelp" placeholder="Enter name" value="<%= Company.Name %>" />
				<small id="emailHelp" class="form-text text-muted">This is required.</small>
			</div>
			<div class="form-group">
				<label for="Name">Address</label>
				<input type="text" class="form-control" id="" aria-describedby="emailHelp" placeholder="Enter name" value="<%= Company.Address %>" />
			</div>
			<div class="form-group">
				<label for="Name">City</label>
				<input type="text" class="form-control" id="" aria-describedby="emailHelp" placeholder="Enter name" value="<%= Company.City %>" />
			</div>
			<div class="form-group">
				<label for="Name">State</label>
				<input type="text" class="form-control" id="" aria-describedby="emailHelp" placeholder="Enter name" value="<%= Company.State %>" />
			</div>
			<div class="form-group">
				<label for="Name">Zip</label>
				<input type="text" class="form-control" id="" aria-describedby="emailHelp" placeholder="Enter name" value="<%= Company.Zip %>" />
			</div>
			<div class="form-group">
				<label for="Name">Phone</label>
				<input type="tel" class="form-control" id="" aria-describedby="emailHelp" placeholder="Enter name" value="<%= Company.Phone %>" />
			</div>
		</form>
	</div>

	<h2>Contacts</h2>
	<h2>Quotes</h2>

	<div class="jumbotron">
		<h1>This is great</h1>
		<p>Welcome to templating using EJS</p>
	</div>
</body>